			$('#id09').css('display', 'block');
			$('#cdslsuccess_msg').replaceWith(
				'<p id="cdslsuccess_msg">' + data['msg'] + '</p>',
			);
		},
		error: function (data) {
			$('.overlay-bg').hide();
			$('#notification').css('display', 'block');
			setInterval(function () {
				$('#notification').css('display', 'none');
			}, 5000);
		},
		dataType: 'json',
	});
}
