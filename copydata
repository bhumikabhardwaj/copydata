var check_nomiee1 = document.getElementById('check_nomiee1');
	var check_nomiee2 = document.getElementById('check_nomiee2');
	var nominee = '';

	if (check_nomiee1.checked == true) {
		nominee = 'yes';
	} else {
		nominee = 'no';
	}
	if (check_nomiee1.checked == false && check_nomiee2.checked == false) {
		alert('please select Nominee option');
	}
